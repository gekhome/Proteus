    <!-- ADDITION FOR HTTP REQUESTS REDIRECTION -->
    <rewrite>
      <rules>
        <rule name="HTTPS redirect" enabled="true" stopProcessing="true">
          <match url="(^proteus/.*)" />
          <conditions>
            <add input="{HTTPS}" pattern="^OFF$" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
    <!-- END OF ADDITION-->
